{
  "manifest_version": 3,

  "name": "Timeowt",
  "description": "TimeMeowt Study Chrome Extension.",
  "version": "1.0",
  "background": {
    "service_worker": "background.js"
  },

  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": ["activeTab", "storage", "alarms", "scripting", "tabs"]
}
